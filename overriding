class Father {
    {
    void surName()
        {
            System.out.println("ch");
    }
    void designation()
    {
        System.out.println("Todday Taker");
     }
    }
    class Child extends Father
    {
        void getName()
        {
            System.out.println("satish");
        }
        void designation()
        {
            System.out.println("java programmer");
        }
    }
    class Overriding
    {
    public static void main(String[] args) {
        Child c1=new Child();
        c1.getName();
        c1.getsurName();
        c1.designation();
    }
    }
}
