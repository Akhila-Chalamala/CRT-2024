import java.util.*;
class Buffer {
    public static void main(String[] args) 
{
Scanner s=new Scanner(System.in);
int i;
int n=s.nextInt();
int a[]=new int[n];
for(i=0;i<n;i++)
{
a[i]=s.nextInt();
}
int t=s.nextInt();
for( i=0;i<n;i++)
{
for(int j=i+1;j<n;j++)
{
if(a[i]+a[j]==t)
{
System.out.println(i+" "+j);
return;
}
}

}
System.out.println("-1");
}
}
